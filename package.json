{
  "name": "dashboard-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:inspect": "nodemon --exec node --inspect=0.0.0.0:9229 --require ts-node/register src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc",
    "prestart": "rm -r dist & tsc",
    "prettier": "prettier . --write",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "prepare": "husky",
    "test-load": "clinic doctor --on-port 'autocannon -m POST localhost:8000' -- node dist/main.js  "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "lint-staged": {
    "**/*": [
      "npm run eslint-fix",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.21.2",
    "inversify": "^6.2.2",
    "reflect-metadata": "^0.2.2",
    "tslog": "^4.9.3",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/express": "^5.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "prettier": "3.5.2",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.25.0"
  }
}
